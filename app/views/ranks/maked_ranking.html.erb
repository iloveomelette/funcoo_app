<div>
  <h2>「作ってみた！」総合ランキング</h2>
  <div>
    <table>
      <tbody>
        <% @high_rank_recipes.each.with_index(1) do |recipe, i| %>
          <tr>
            <td>第<%= i %>位</td>
            <td>
              <div>
                <%= render "recipes/recipe", recipe: recipe %>
              </div>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
